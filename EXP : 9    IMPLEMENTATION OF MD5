#include <stdio.h>
#include <string.h>
unsigned int simpleMD5(const char *str) {
    unsigned int hash = 0xABCDEF01;
    for (int i = 0; str[i] != '\0'; i++) {
        hash ^= ((hash << 5) + str[i] + (hash >> 2));
    }
    return hash;
}

int main() {
    char msg[256];
    printf("Enter message: ");
    fgets(msg, sizeof(msg), stdin);
    msg[strcspn(msg, "\n")] = 0;

    unsigned int h1 = simpleMD5(msg);
    unsigned int h2 = simpleMD5(msg + 1);
    unsigned int h3 = simpleMD5(msg + 2);
    unsigned int h4 = simpleMD5(msg + 3);

    printf("Pseudo MD5 hash: %08x%08x%08x%08x\n", h1, h2, h3, h4);
    return 0;
}
